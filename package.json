{
  "name": "e-ink-display-server",
  "version": "1.4.3",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "build": "rm -rf dist/* && npm run build-ts",
    "serve": "node dist/server.js",
    "watch": "nodemon --watch src --ext js,ts --exec 'node -r ts-node/register' src/server.ts",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "nodemon --watch src --ext js,ts --exec 'node -r ts-node/register --inspect' src/server.ts",
    "preversion": "npm run build && git add -A dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tfohlmeister/eink-display-server"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.18.36",
    "@types/node-fetch": "^2.5.7",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "chokidar": "^3.4.2",
    "express": "^4.17.1",
    "heic-decode": "^1.1.1",
    "jimp": "^0.16.0",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2"
  }
}
